{
  "name": "react-input-message",
  "description": "unopinionated form input annotation",
  "version": "0.9.0",
  "repository": {
    "url": "git://github.com/jquense/react-input-message.git"
  },
  "author": "@monasticpanic Jason Quense",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jquense/react-input-message/issues"
  },
  "main": "index.js",
  "scripts": {
    "test": "karma start --single-run",
    "tdd": "karma start",
    "example": "rimraf ./example/bundle.js && webpack-dev-server --config webpack/example-config.babel.js",
    "build:example": "webpack --config webpack/example-config.babel.js",
    "build": "npm run test && npm run build:example && babel src --out-dir lib",
    "release": "release"
  },
  "dependencies": {
    "classnames": "^1.1.4",
    "promise": "^7.0.3",
    "topeka": "^0.1.3"
  },
  "peerDependencies": {
    "react": "^0.14.0 || ^15.0.0"
  },
  "release-script": {
    "defaultDryRun": "false",
    "altPkgRootFolder": "lib"
  }
}
